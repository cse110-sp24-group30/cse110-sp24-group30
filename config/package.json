{
  "name": "your-project",
  "version": "1.0.0",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --passWithNoTests",
    "build": "node --experimental-vm-modules node_modules/jest/bin/jest.js --passWithNoTests",
    "lint": "eslint .. --config ./eslint.config.js",
    "docs": "npx jsdoc -c jsdoc.json"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "eslint": "^9.2.0",
    "globals": "^15.2.0",
    "jest": "^29.7.0",
    "jest-puppeteer": "^10.0.1",
    "jsdoc": "^4.0.3"
  },
  "type": "module",
  "jest": {
    "preset": "jest-puppeteer",
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)+(spec|test).js?(x)"
    ],
    "transform": {},
    "verbose": true
  }
}

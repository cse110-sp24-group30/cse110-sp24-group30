{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": ["../source/calendar", "../source/journal", "../source/to_do"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "opts": {
        "destination": "../docs"
      },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}